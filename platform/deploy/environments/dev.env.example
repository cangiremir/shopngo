# Development environment template (non-secret values only)
ASPNETCORE_ENVIRONMENT=Development
OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4318
OTEL_PROM_METRICS_ENABLED=true
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
ORDER_RABBITMQ_THROUGHPUT_LEVEL=Balanced
ORDER_RABBITMQ_ENDPOINT_STOCK_RESERVED_LEVEL=Balanced
ORDER_RABBITMQ_ENDPOINT_STOCK_REJECTED_LEVEL=Balanced
STOCK_RABBITMQ_THROUGHPUT_LEVEL=Balanced
STOCK_RABBITMQ_ENDPOINT_ORDER_CREATED_LEVEL=Conservative
NOTIFICATION_RABBITMQ_THROUGHPUT_LEVEL=Aggressive
REDIS_HOST=redis
REDIS_PORT=6379
ORDER_DB_HOST=order-db
ORDER_DB_PORT=5432
ORDER_DB_USER=postgres
ORDER_DB_PASSWORD=postgres
STOCK_DB_HOST=stock-db
STOCK_DB_PORT=5432
STOCK_DB_USER=postgres
STOCK_DB_PASSWORD=postgres
NOTIFICATION_DB_HOST=notification-db
NOTIFICATION_DB_PORT=5432
NOTIFICATION_DB_USER=postgres
NOTIFICATION_DB_PASSWORD=postgres
STOCK_CONCURRENCY_MODE=Hybrid
STOCK_CONCURRENCY_HYBRID_ENABLED=true
STOCK_CONCURRENCY_HYBRID_CANARY_PERCENT=15
STOCK_CONCURRENCY_HYBRID_LOW_STOCK_THRESHOLD=10
STOCK_CONCURRENCY_HYBRID_CONSERVATIVE_ON_GUARDRAIL_FAILURE=true
STOCK_GUARDRAIL_ENABLED=true
STOCK_GUARDRAIL_FAIL_OPEN=true
STOCK_GUARDRAIL_KEY_PREFIX=shopngo:stock
STOCK_GUARDRAIL_MAX_IN_FLIGHT_PER_SKU=32
STOCK_GUARDRAIL_IN_FLIGHT_TTL_SECONDS=30
STOCK_GUARDRAIL_HOT_SKU_WINDOW_SECONDS=60
STOCK_GUARDRAIL_HOT_SKU_ENTER_THRESHOLD=100
STOCK_GUARDRAIL_HOT_SKU_EXIT_THRESHOLD=60
STOCK_GUARDRAIL_HOT_SKU_TTL_SECONDS=300
ORDER_SERVICE_IMAGE=ghcr.io/your-org/shopngo-order-service:dev
STOCK_SERVICE_IMAGE=ghcr.io/your-org/shopngo-stock-service:dev
NOTIFICATION_SERVICE_IMAGE=ghcr.io/your-org/shopngo-notification-service:dev
IMAGE_TAG=dev
