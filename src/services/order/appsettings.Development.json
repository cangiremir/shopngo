{
  "DatabaseStartup": {
    "Mode": "Migrate",
    "MaxRetryCount": 15,
    "RetryDelaySeconds": 2
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMq": {
    "PrefetchCount": 32,
    "Throughput": {
      "Level": "Balanced"
    },
    "PublisherConfirmsEnabled": false
  },
  "OpenTelemetry": {
    "Metrics": {
      "Prometheus": {
        "Enabled": true
      }
    }
  },
  "OutboxDispatcher": {
    "PollIntervalMs": 200,
    "BatchSize": 200
  }
}
